    "jszip": "~3.10.1",
    "knex": "~2.3.0",
    "live-directory": "~2.3.2",
    "markdown-it": "~13.0.1",
    "node-fetch": "~2.6.7",
    "nunjucks": "~3.2.3",
    "parse-duration": "~1.0.2",
    "randomstring": "~1.2.3",
    "range-parser": "~1.2.1",
    "rate-limiter-flexible": "~2.4.1",
    "search-query-parser": "~1.6.0",
    "sharp": "~0.30.7",
    "systeminformation": "~5.12.11"
