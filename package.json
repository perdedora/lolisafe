    "eslint-plugin-promise": "~6.1.1",
    "gulp": "~4.0.2",
    "gulp-buble": "~0.9.0",
    "gulp-cli": "~2.3.0",
    "gulp-dart-sass": "~1.0.2",
    "gulp-eslint-new": "~1.7.0",
    "gulp-if": "~3.0.0",
    "gulp-nodemon": "~2.5.0",
    "gulp-postcss": "~9.0.1",
    "gulp-replace": "~1.1.3",
    "gulp-sourcemaps": "~3.0.0",
    "gulp-terser": "~2.1.0",
    "postcss": "~8.4.19",
